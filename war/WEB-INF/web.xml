<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

<!-- Servlets -->
  <servlet>
    <servlet-name>JDOServiceImpl</servlet-name>
    <servlet-class>ubc.cs.cpsc310.rackbuddy.server.JDOServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>JDOServiceImpl</servlet-name>
    <url-pattern>/rackbuddy/jdoService</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>GeoParserServiceImpl</servlet-name>
    <servlet-class>ubc.cs.cpsc310.rackbuddy.server.GeoParserServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>GeoParserServiceImpl</servlet-name>
    <url-pattern>/rackbuddy/GeoParserService</url-pattern>
  </servlet-mapping>
  
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>RackBuddy.html</welcome-file>
  </welcome-file-list>


</web-app>
